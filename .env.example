# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Supabase Service Role Key (for admin operations - keep this secret!)
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Application Environment
NEXT_PUBLIC_APP_ENV=production
NEXT_PUBLIC_APP_NAME=DukaaOn Admin Console
NEXT_PUBLIC_APP_VERSION=1.0.0

# Admin Configuration
NEXT_PUBLIC_ADMIN_DOMAIN=admin.dukaaon.in
NEXT_PUBLIC_MAIN_APP_DOMAIN=dukaaon.in

# Security
NEXTAUTH_SECRET=your-nextauth-secret-here
NEXTAUTH_URL=https://admin.dukaaon.in

# Analytics (Optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=

# Monitoring (Optional)
NEXT_PUBLIC_SENTRY_DSN=

# Azure AI Vision (Optional - for OCR functionality)
# Get these values from your Azure Computer Vision resource:
# 1. NEXT_PUBLIC_AZURE_ENDPOINT: Your Azure Computer Vision endpoint URL
# 2. NEXT_PUBLIC_AZURE_API_KEY: Your Azure Computer Vision API key
# 3. NEXT_PUBLIC_AZURE_REGION: Your Azure region (default: eastus)
NEXT_PUBLIC_AZURE_ENDPOINT=
NEXT_PUBLIC_AZURE_API_KEY=
NEXT_PUBLIC_AZURE_REGION=eastus

# OCR Provider Configuration
NEXT_PUBLIC_OCR_PROVIDER=aws